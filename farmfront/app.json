{
  "expo": {
    "name": "Farm-Manager",
    "description": "This app helps Tele-farmers have a visual of what happens on their farm",
    "slug": "farm-manager",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.refactory.farmmanager"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "Farm-manager",
  "name": "Farm-Manager"
}
